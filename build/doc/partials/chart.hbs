{{#if unicodeHex}}
<li>
    <span class="sb-glyphicons">&#x{{{unicodeHex}}};&nbsp;</span>
    <span class="glyphinfo">&#x{{unicodeHex}}; (x{{unicodeHex}})</span> 
</li>
{{else}}
<li>
    <span class="sb-glyphicons">&nbsp;</span>
    <span class="glyphinfo">No Unicode</span> 
    <span class="glyphinfo">Name: {{name}}</span> 
</li>
{{/if}}